{
  "name": "@w4rlock/semantic-release-plugin-decorators",
  "version": "0.0.2",
  "main": "src/index.js",
  "repository": "git@github.com:w4rlock/semantic-release-plugin-decorators.git",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write --single-quote --trailing-comma es5",
    "format:all": "yarn format \"./**/*.js\"",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "license": "MIT",
  "peerDependencies": {
    "semantic-release": ">=11"
  },
  "devDependencies": {
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.4",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.1",
    "semantic-release-github-pr": "^6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn format"
    ]
  }
}
